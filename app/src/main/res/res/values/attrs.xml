<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="dimension"/>
    </declare-styleable>   
     <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
    </declare-styleable>
    
     
      <declare-styleable name="MultiChoiceExpandableListView">
        <attr name="groupChoiceMode">
            <enum name="multi" value="2" />
            <enum name="one" value="1" />
            <enum name="none" value="0" />
        </attr>
        <attr name="childChoiceMode">
            <enum name="multi" value="2" />
            <enum name="onePerGroup" value="14" />
            <enum name="one" value="1" />
            <enum name="none" value="0" />
        </attr>
        <attr name="oneItemChoice" format="boolean" />
        <attr name="checkChildrenWithGroup" format="boolean" />
    </declare-styleable>
    
      <declare-styleable name="ShownEdittext">
        <attr name="android:inputType"/>
        <attr name="android:hint"/>
        <attr name="android:enabled"/>
    </declare-styleable>
     
</resources>